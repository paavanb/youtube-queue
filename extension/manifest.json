{
    "name": "YouTube Queue",
    "description": "Create on-the-fly video playlists and search while watching",
    "version": "1",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": [
            "js/material.js",
            "js/youtube-queue.js"
          ],
          "css": ["css/material.css", "css/styles.css"]
        }
    ]
}
